{
  "name": "create-events-bot",
  "version": "1.3.1",
  "description": "Create events in Telegram groups",
  "main": "index.js",
  "scripts": {
    "start": "yarn install && yarn lint && ts-node index.ts",
    "lint": "eslint .",
    "deploy": "./scripts/deploy.sh",
    "switch_to_dev": "export $(cat .env | xargs) && wget --spider https://api.telegram.org/bot$PROD_BOT_TOKEN/setWebhook?url= --delete-after && node index.js"
  },
  "author": "David Ziegler",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.0",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "node-telegram-bot-api": "^0.52.0",
    "node-telegram-keyboard-wrapper": "^3.0.0",
    "pg": "^7.12.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@types/node-telegram-bot-api": "^0.51.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}